                                    </Separator>
                                    <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                                       <children>
                                          <HBox alignment="CENTER_LEFT" spacing="10.0">
                                             <children>
                                                <Label text="Instagram token:" />
                                                <TextField fx:id="instaToken" promptText="Paste instagram token here..." HBox.hgrow="ALWAYS" />
                                             </children>
                                             <VBox.margin>
                                                <Insets bottom="10.0" top="10.0" />
                                             </VBox.margin>
                                          </HBox>
                                          <Separator prefWidth="200.0">
                                             <VBox.margin>
                                                <Insets bottom="5.0" top="5.0" />
                                             </VBox.margin>
                                          </Separator>
                                          <HBox alignment="CENTER_LEFT" spacing="10.0">
                                             <children>
                                                <Label text="Storage folder:" />
                                                <TextField fx:id="instaStorageFolderTF" HBox.hgrow="ALWAYS" />
                                                <Button fx:id="instaStorageFolderButton" mnemonicParsing="false" onAction="#onInstaStorageFolder" text="..." />
                                             </children>
                                             <VBox.margin>
                                                <Insets bottom="10.0" top="10.0" />
                                             </VBox.margin>
                                          </HBox>
                                          <Separator prefWidth="200.0">
                                             <VBox.margin>
                                                <Insets bottom="5.0" top="5.0" />
                                             </VBox.margin>
                                          </Separator>
                                          <HBox alignment="CENTER_LEFT" spacing="10.0">
                                             <children>
                                                <Label text="Saved images name prefix:" />
                                                <TextField fx:id="instaNamePrefix" HBox.hgrow="ALWAYS" />
                                             </children>
                                             <VBox.margin>
                                                <Insets bottom="10.0" top="10.0" />
                                             </VBox.margin>
                                          </HBox>
                                          <Separator prefWidth="200.0">
                                             <VBox.margin>
                                                <Insets bottom="5.0" top="5.0" />
                                             </VBox.margin>
                                          </Separator>
                                          <HBox spacing="10.0">
                                             <children>
                                                <Label text="Max number of fetched images:" />
                                                <TextField fx:id="instaNumberOfFetchedImages" HBox.hgrow="ALWAYS" />
                                             </children>
                                             <VBox.margin>
                                                <Insets bottom="10.0" top="10.0" />
                                             </VBox.margin>
                                          </HBox>
                                       </children>
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
                                       </padding>
                                    </VBox>
                                 </children></HBox>
                              <HBox alignment="CENTER" prefWidth="200.0">
                                 <children>
                                    <Button fx:id="instaRunButton" mnemonicParsing="false" onAction="#onInstaRun" prefHeight="30.0" prefWidth="150.0" text="Run" />
                                    <ProgressIndicator fx:id="instaProgressIndicator" visible="false" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children></AnchorPane>
                    </content>
